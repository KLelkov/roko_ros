<?xml version="1.0" encoding="UTF-8"?>

<launch>
  <!--launch robot model to iteract with-->
  <node pkg="roko_robot" type="roko_node.py" name="robot"/>
<!--launch rviz for visualization-->
  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find robot_software)/navigation.rviz"/>
 
<!-- launch control algo -->
  <node pkg="robot_software" type="py_control_node.py" name="control_node" output="screen"/>
</launch>
